PREFIX : <https://linked.data.gov.au/def/mineral-occurrence/>
PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX geofh: <https://linked.data.gov.au/def/mineral-occurrence/>
PREFIX ont: <https://linked.data.gov.au/def/geofh>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:1.0 schema:name "1.0" .

ont:
    a owl:Ontology ;
    owl:versionIRI :1.0 ;
    schema:codeRepository "https://github.com/Kurrawong/gswa-supermodel/"^^xsd:anyURI ;
    schema:copyrightHolder <https://linked.data.gov.au/org/gswa> ;
    schema:creator <https://kurrawong.ai> ;
    schema:dateCreated "2025-01-15"^^xsd:date ;
    schema:dateModified "2025-09-17"^^xsd:date ;
    schema:dateIssued "2025-06-15"^^xsd:date ;
    schema:description """A model of the occurrence of minerals""" ;
    schema:license <http://purl.org/NET/rdflicense/cc-by4.0> ;
    schema:name "Mine Model" ;
    schema:publisher <https://linked.data.gov.au/org/gswa> ;
    schema:version "1.0" ;
    skos:historyNote """1.0 - Sep 2025 - First complete version released into the GSWA Knowledge Graph""" ;
    schema:hasPart :ResourceOccurrence ;
.

:ResourceOccurrence
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf geo:Feature ;
    schema:description "The occurrence of a commercially potential resource" ;
    schema:name "Resource Occurrence" ;
   skos:example """
ex:ro-x
    a rom:ResourceOccurrence ;
    geo:hasQualifiedGeometry [
        a ego:QualifiedGeometry ;
        geo:asWKT "POLYGON (( ... ))"^^geo:wktLiteral ;
        ego:evidence [
            a sosa:Observation ;
            sosa:observedProperty obsprops:amount-of-gold ;
            sosa:hasResult [
                a sosa:Result ;
                rdf:value 2.0 ;
                qudt:unit unit:PPM ;
            ] ;
        ] ;
    ] ,
    ...
.
""" ;
    skos:historyNote """First defined in this model""" ;
    skos:scopeNote """This class is being used to represent features which are not obviously entirely natural or man-made, for example, an area of bauxite ore that is commercial because of both its concentration in an area and that area's proximity to transport infrastructure.

This class is expected to be used with complex geometry objects representing instance's locations, for example, SUFF Fuzzy Geometry objects.""" ;
.